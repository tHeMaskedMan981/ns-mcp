{
  "name": "ns-mcp-server",
  "version": "1.0.0",
  "description": "MCP server for Network School Luma events",
  "type": "module",
  "main": "build/index.js",
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "start": "node build/index.js",
    "start:http": "node build/index-http.js",
    "start:sse": "node build/index-sse.js",
    "dev:http": "tsc && node build/index-http.js",
    "dev:sse": "tsc && node build/index-sse.js",
    "test": "tsc && node build/test.js",
    "test:wiki": "tsc && node build/test-wiki.js",
    "test:search": "tsc && node build/test-search.js"
  },
  "keywords": [
    "mcp",
    "network-school",
    "luma",
    "events"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.19.1",
    "axios": "^1.12.2",
    "cors": "^2.8.5",
    "date-fns": "^4.1.0",
    "date-fns-tz": "^3.2.0",
    "dotenv": "^16.4.5",
    "express": "^4.21.2"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^5.0.0",
    "@types/node": "^22.10.1",
    "typescript": "^5.7.2"
  }
}
